  flex-direction: row;
  margin-top: 25px;
  margin-bottom: 15px;
  text-indent: 180px;
  font-size: 18px;
  /*font-family: Helvetica;*/
}

.EBstyle ul{
  list-style-type:none;
}

.EBstyle ul li{
  margin: 10px 0;
}

.TSstyle {
  display: flex;
  flex-direction: column;
  float: left;
  width: 50%;
  margin-top: 24px;
  margin-bottom: 10px;
  font-size: 15px;
  
  /*font-family: Helvetica;*/
}

.TSstyle p{
  font-size: 20px;
  margin-left: 125px;
}

.TSstyle ul{
